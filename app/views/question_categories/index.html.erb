<h1>Listing question_categories</h1>

<table>
  <tr>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_categories.each do |question_category| %>
  <tr>
    <td><%= question_category.value %></td>
    <td><%= link_to 'Show', question_category %></td>
    <td><%= link_to 'Edit', edit_question_category_path(question_category) %></td>
    <td><%= link_to 'Destroy', question_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question category', new_question_category_path %>
